<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.nazeer.sessionmonitorexample.MainActivity">

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="monitored activity"
        android:id="@+id/button1"
        android:background="@drawable/round_corner_item"
        android:padding="10dp"
        android:layout_margin="10dp"
        android:textColor="@android:color/white"
        />
    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="activity with session object"
        android:id="@+id/button2"
        android:background="@drawable/round_corner_item"
        android:padding="10dp"
        android:layout_margin="10dp"
        android:textColor="@android:color/white"
        />
    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="fragment activity"
        android:id="@+id/button3"
        android:background="@drawable/round_corner_item"
        android:padding="10dp"
        android:layout_margin="10dp"
        android:textColor="@android:color/white"
        />
    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="show Data"
        android:id="@+id/buttonShowaData"
        android:background="@drawable/round_corner_item"
        android:padding="10dp"
        android:layout_margin="10dp"
        android:textColor="@android:color/white"
        />
</LinearLayout>
