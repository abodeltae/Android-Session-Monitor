<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nazeer.sessionmonitorexample">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExampleMonitoredActivity" />
        <activity android:name=".ExampleActivityWithSessionObject" />
        <activity android:name=".FragmentActivity" />
        <activity android:name=".ShowDataActivity" />
        <activity android:name=".ReportActivity"></activity>
    </application>

</manifest>